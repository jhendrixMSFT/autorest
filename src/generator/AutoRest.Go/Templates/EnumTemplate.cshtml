@using AutoRest.Go;
@using System.Linq;

@inherits AutoRest.Go.Template<AutoRest.Go.TemplateModels.EnumTemplateModel>
@{
    var constants = Model.Constants.OrderBy(v => v.Name);
    var modelName = GoCodeNamer.CamelCase(Model.Name);
    var modelPhrase = Model.Name.ToPhrase();
}

@WrapComment("// ", Model.Documentation)
type @Model.Name string

@EmptyLine
const (
@foreach (var c in constants)
{
    @:@WrapComment("// ", string.Format("{0} specifies the {1} state for {2}.", c.Name, c.Name.ToPhrase(), modelPhrase))
    @:@(c.Name) @(Model.Name) = "@(c.Value)"
}
)
@EmptyLine
